<table class="table table-hover">
    <thead>
      <tr>
        <th>Date</th>
        <th>Category</th>
        <th>Due Date</th>
        <th>Goal</th>
      </tr>
    </thead>
    <tbody id="goals_table">
      <% goals.forEach((goal) => {%>
      <tr>
        <td><%= goal.time_submitted %></td>
        <td><%= goal.category %></td>
        <td><%= goal.dueDate %></td>
        <td><%= goal.goal %></td>
        <td>
        <form method="POST" action="/complete/<%= goal.goal_id %>">
          <button>
            âœ…
          </button>
        </form>
        </td>
        <td>
        <form method="POST" action="/delete/<%= goal.goal_id %>">
          <button>
            DELETE
          </button>
        </form>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
